function scrNumberToGrade(argument0) {
	switch (argument0) {
	    case 0: return "";
	    case 1: return "F-";
	    case 2: return "F";
	    case 3: return "F+";
	    case 4: return "D-";
	    case 5: return "D";
	    case 6: return "D+";
	    case 7: return "C-";
	    case 8: return "C";
	    case 9: return "C+";
	    case 10: return "B-";
	    case 11: return "B";
	    case 12: return "B+";
	    case 13: return "A-";
	    case 14: return "A";
	    case 15: return "A+";
	    default: return "";
	}



}
