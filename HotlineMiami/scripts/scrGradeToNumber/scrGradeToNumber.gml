function scrGradeToNumber(argument0) {
	switch (argument0) {
	    case "F-": return 1;
	    case "F": return 2;
	    case "F+": return 3;
	    case "D-": return 4;
	    case "D": return 5;
	    case "D+": return 6;
	    case "C-": return 7;
	    case "C": return 8;
	    case "C+": return 9;
	    case "B-": return 10;
	    case "B": return 11;
	    case "B+": return 12;
	    case "A-": return 13;
	    case "A": return 14;
	    case "A+": return 15;
    
	    default: return 0;
	}



}
